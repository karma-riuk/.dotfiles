dunst - $XDG_CONFIG_HOME/dunst/dunstrc
picom - $XDG_CONFIG_HOME/picom/picom.conf
polybar - $XDG_CONFIG_HOME/polybar/config
profile - $HOME/.profile
qtile - $XDG_CONFIG_HOME/qtile/config.py
qutebrowser - $XDG_CONFIG_HOME/qutebrowser/config.py
termite - $XDG_CONFIG_HOME/termite/config
tmux - $XDG_CONFIG_HOME/tmux/tmux.conf
vconf - $XDG_CONFIG_HOME/vconf/list
vim - $XDG_CONFIG_HOME/vim/vimrc
zsh - $XDG_CONFIG_HOME/zsh/.zshrc
zsh aliases - $XDG_CONFIG_HOME/zsh/aliases
zsh environmental - $XDG_CONFIG_HOME/zsh/.zshenv
